[run]
source=.

[report]
fail_under=100

exclude_lines =
    pragma: no cover
    pragma: todo cover
    def __str__
    def __unicode__
    def __repr__

omit=
    */migrations/*
    */apps.py
    */admin.py
    manage.py
    setup.py
    {{cookiecutter.project_name}}/settings_*.py
    {{cookiecutter.project_name}}/wsgi.py

show_missing = True
